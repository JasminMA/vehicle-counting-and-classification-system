{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "tsc --noEmit",
    "clean": "rm -rf cdk.out && rm -rf node_modules && rm -f *.tsbuildinfo",
    "synth": "cdk synth",
    "synth:dev": "cdk synth -c environment=dev",
    "synth:prod": "cdk synth -c environment=prod",
    "deploy:dev": "cdk deploy --all -c environment=dev",
    "deploy:prod": "cdk deploy --all -c environment=prod",
    "diff:dev": "cdk diff -c environment=dev",
    "diff:prod": "cdk diff -c environment=prod"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.1017.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.198.0",
    "constructs": "^10.0.0"
  }
}
